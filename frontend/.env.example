# Frontend Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_BACKEND_URL=http://localhost:5000

# App Configuration
REACT_APP_NAME=FinTrack
REACT_APP_VERSION=1.0.0
REACT_APP_ENV=development

# Authentication
REACT_APP_JWT_SECRET=your_jwt_secret_here

# External Services
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Social Login
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# File Upload
REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# PWA Configuration
REACT_APP_PWA_ENABLED=true

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SOCIAL_LOGIN=false
REACT_APP_ENABLE_PAYMENTS=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Demo Mode
REACT_APP_DEMO_MODE=true
REACT_APP_DEMO_USER_EMAIL=demo@example.com
REACT_APP_DEMO_USER_PASSWORD=demo123

# Error Reporting
REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Performance Monitoring
REACT_APP_NEW_RELIC_APP_ID=your_new_relic_app_id
REACT_APP_NEW_RELIC_LICENSE_KEY=your_new_relic_license_key

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.yourdomain.com

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#4a90e2
REACT_APP_SECONDARY_COLOR=#357abd
REACT_APP_ACCENT_COLOR=#f59e0b

# Currency Settings
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_CURRENCY_SYMBOL=$

# Date Format
REACT_APP_DATE_FORMAT=MM/DD/YYYY
REACT_APP_TIMEZONE=America/New_York

# API Rate Limiting
REACT_APP_API_RATE_LIMIT=100
REACT_APP_API_TIMEOUT=30000

# Cache Configuration
REACT_APP_CACHE_TTL=3600000
REACT_APP_ENABLE_SERVICE_WORKER=true

# Development
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true
